#include <bits/stdc++.h>
#include<string>
using namespace std;

int main() {
    // basicIO();
    // Your cpp code here
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        int size = s.size();
        if(int(s[size-1])%2==0) cout<<"even\n";
            else cout<<"odd\n";
    }
    return 0;
}
