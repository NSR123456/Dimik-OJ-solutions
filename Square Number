#include <bits/stdc++.h>
#include<cmath>
using namespace std;

int main() {
    // basicIO();
    // Your cpp code here
    int t;
    cin>>t;
    for(int k=1;k<=t;k++){
        int n;
        cin>>n;
        int b = sqrt(n);
        if(b*b==n) cout<<"YES\n";
        else cout<<"NO\n";
    }
    return 0;
}
