#include <bits/stdc++.h>
using namespace std;



int main() {
    // basicIO();
    // Your cpp code here
    int t;
    cin>>t;
    for(int k=1;k<=t;k++){
        long long n;
        long long fact=1;
        cin>>n;
        
        for(int i=1;i<=n;i++)
            fact *=i;
    
        cout<<fact<<'\n';
}
    return 0;
}
