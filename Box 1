#include <bits/stdc++.h>
using namespace std;


int main() {
    // basicIO();
    // Your cpp code here
    int t;
    cin>>t;
    for(int case=1;case<=t;case++){
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
        for(int j=1;j<=n;j++){
            cout<<"*";
        }
        cout<<"\n";
    }
        if(case!=t) {cout<<'\n';}
        }
    return 0;
}
