#include <bits/stdc++.h>
#include<cmath>
using namespace std;


int main() {
    // basicIO();
    // Your cpp code here
    int t;
    cin>>t;
    
    for(int i=1;i<=t;i++){
        int n;
        cin>>n;
        cout<<"Case "<<i<<": ";
        for(int i=1; i<n;i++){
            if(n%i==0) {
                cout<<i;
                cout<<" ";}
        
    }
        cout<<n<<'\n';}
    return 0;
}
