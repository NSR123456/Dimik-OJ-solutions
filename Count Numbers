#include <bits/stdc++.h>
#include<string>
using namespace std;



int main() {
    // basicIO();
    // Your cpp code here
    int t;
    cin>>t;
    while(t--){

        int count =0;
        string s;
        //getline();
        getline(cin>>ws, s);
        for(int i=0;i<s.size();i++){
            if(s[i]==' ' & s[i+1]!= ' ') count++;
        }
        count++;
        cout<<count<<'\n';
    }
    return 0;
}
